{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "p4d": {
      "root": "apps/p4d/",
      "sourceRoot": "apps/p4d/src",
      "projectType": "application",
      "prefix": "p4d",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/p4d",
            "index": "apps/p4d/src/index.html",
            "main": "apps/p4d/src/main.ts",
            "polyfills": "apps/p4d/src/polyfills.ts",
            "tsConfig": "apps/p4d/tsconfig.app.json",
            "assets": ["apps/p4d/src/favicon.ico", "apps/p4d/src/assets"],
            "styles": ["apps/p4d/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/p4d/src/environments/environment.ts",
                  "with": "apps/p4d/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "p4d:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "p4d:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "p4d:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/p4d/src/test.ts",
            "polyfills": "apps/p4d/src/polyfills.ts",
            "tsConfig": "apps/p4d/tsconfig.spec.json",
            "karmaConfig": "apps/p4d/karma.conf.js",
            "styles": ["apps/p4d/src/styles.scss"],
            "scripts": [],
            "assets": ["apps/p4d/src/favicon.ico", "apps/p4d/src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/p4d/tsconfig.app.json",
              "apps/p4d/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "p4d-e2e": {
      "root": "apps/p4d-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/p4d-e2e/protractor.conf.js",
            "devServerTarget": "p4d:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "p4d:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/p4d-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "p4d-cy": {
      "root": "apps/p4d-cy/",
      "sourceRoot": "apps/p4d-cy/src",
      "projectType": "application",
      "prefix": "p4d",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/p4d-cy",
            "index": "apps/p4d-cy/src/index.html",
            "main": "apps/p4d-cy/src/main.ts",
            "polyfills": "apps/p4d-cy/src/polyfills.ts",
            "tsConfig": "apps/p4d-cy/tsconfig.app.json",
            "assets": ["apps/p4d-cy/src/favicon.ico", "apps/p4d-cy/src/assets"],
            "styles": ["apps/p4d-cy/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/p4d-cy/src/environments/environment.ts",
                  "with": "apps/p4d-cy/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "p4d-cy:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "p4d-cy:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "p4d-cy:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/p4d-cy/src/test.ts",
            "polyfills": "apps/p4d-cy/src/polyfills.ts",
            "tsConfig": "apps/p4d-cy/tsconfig.spec.json",
            "karmaConfig": "apps/p4d-cy/karma.conf.js",
            "styles": ["apps/p4d-cy/src/styles.scss"],
            "scripts": [],
            "assets": ["apps/p4d-cy/src/favicon.ico", "apps/p4d-cy/src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/p4d-cy/tsconfig.app.json",
              "apps/p4d-cy/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "p4d-cy-e2e": {
      "root": "apps/p4d-cy-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@nrwl/builders:cypress",
          "options": {
            "cypressConfig": "apps/p4d-cy-e2e/cypress.json",
            "tsConfig": "apps/p4d-cy-e2e/tsconfig.e2e.json",
            "devServerTarget": "p4d-cy:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "p4d-cy:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/p4d-cy-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics"
  },
  "schematics": {
    "@nrwl/schematics:application": {
      "style": "scss"
    },
    "@nrwl/schematics:library": {
      "style": "scss"
    }
  },
  "defaultProject": "p4d"
}
